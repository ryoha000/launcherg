{
  "manifest_version": 3,
  "name": "Launcherg DL Store Sync",
  "version": "1.0.0",
  "description": "Automatically sync your purchased games from DMM and DLsite to Launcherg",
  "key": "",
  "permissions": [
    "nativeMessaging",
    "activeTab",
    "storage",
    "notifications",
    "alarms"
  ],
  "host_permissions": [
    "https://games.dmm.co.jp/*",
    "https://play.dlsite.com/*"
  ],
  "background": {
    "service_worker": "background/background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "https://games.dmm.co.jp/*"
      ],
      "js": [
        "chunks/messages_pb-G4UeCIP6.js",
        "content-scripts/dmm-extractor.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "https://play.dlsite.com/*"
      ],
      "js": [
        "chunks/messages_pb-G4UeCIP6.js",
        "content-scripts/dlsite-extractor.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "Launcherg DL Store Sync"
  },
  "options_page": "popup/popup.html"
}
